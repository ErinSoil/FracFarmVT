data.stats <- data %>% 
  group_by(Type.x) %>%
  summarize (n=n(),
            mean=mean(years.since.till, na.rm = TRUE), 
            min=min(years.since.till, na.rm = TRUE), 
            max=max(years.since.till, na.rm = TRUE), 
            sd=sd(years.since.till, na.rm = TRUE),
            mean=mean(years.since.till, na.rm = TRUE), 
